{
  "manifest_version": 3,
  "name": "oneTwoOne Lockdown",
  "short_name": "oneTwoOne",
  "description": "Prevents students from logging into other student Chromebooks.",
  "version": "3.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "minimum_chrome_version": "88",
  "storage": {
    "managed_schema": "schema.json"
  },
  "permissions": [
    "identity",
    "identity.email",
    "enterprise.deviceAttributes",
    "activeTab",
    "webRequest",
    "declarativeNetRequestWithHostAccess",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}